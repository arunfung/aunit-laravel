{
    "name": "arunfung/aunit-laravel",
    "description": "A simplified version of the laravel framework unit test",
    "license": "MIT",
    "authors": [
        {
            "name": "Arun Fung",
            "email": "arunfung@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^6.2|7.*|8.*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "ArunFung\\AunitLaravel\\": "src/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "ArunFung\\AunitLaravel\\Providers\\AunitServiceProvider"
            ]
        }
    }
}
